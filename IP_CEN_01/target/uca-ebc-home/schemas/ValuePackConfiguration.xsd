<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:tns="http://hp.com/uca/expert/config" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="2.0" targetNamespace="http://hp.com/uca/expert/config" elementFormDefault="qualified">

    <xs:element name="valuePackConfiguration">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="scenarios" type="tns:scenarios" />
                <xs:element name="mediationFlows" type="tns:mediationFlows" minOccurs="0" />
                <xs:element name="dbFlows" type="tns:dbFlows" minOccurs="0" />
            </xs:sequence>
            <xs:attribute name="name" type="xs:string" use="required" />
            <xs:attribute name="version" type="xs:string" use="required" />
            <xs:attribute name="__tooltip" type="xs:string" fixed="Configuration of a ValuePack" use="optional">
                <xs:annotation>
                    <xs:appinfo>
                        <jxb:property fixedAttributeAsConstantProperty="true" />
                    </xs:appinfo>
                </xs:annotation>
            </xs:attribute>
        	<xs:attribute name="remoteAccess" type="xs:boolean" use="optional" default="false" />
        </xs:complexType>

    </xs:element>


    <xs:complexType name="scenarios">
        <xs:sequence>
            <xs:element name="scenario" type="tns:scenarioPolicies" minOccurs="1" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="__tooltip" type="xs:string" fixed="List of Scenarios defined in the Value Pack" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="scenarioPolicies">
        <xs:sequence>
            <xs:element name="automaticRefreshOnConfigurationChange" type="xs:boolean" minOccurs="0" default="false">
            </xs:element>

            <xs:element name="actionRetractedAutomaticallyWhenCompleted" type="xs:boolean" minOccurs="0" default="true">
            </xs:element>

            <xs:element name="alarmEligibilityPolicy" type="xs:string" minOccurs="0" default="true">
            </xs:element>
            <xs:element name="asyncActionPeriod" type="xs:long" minOccurs="0" default="1000" />
            <xs:element name="clockTypeMode" type="tns:clockMode" minOccurs="0" default="NORMAL" />
            <xs:element name="eligibleForBroadcast" type="xs:boolean" minOccurs="0" default="true" />
            <xs:element name="filterFiles" type="tns:filterFiles" minOccurs="0" />
            <xs:element name="filterTagsFile" type="xs:string" minOccurs="0" />
            <xs:element name="mapperFiles" type="tns:mapperFiles" minOccurs="0" />
            <xs:element name="fireAllRulePeriod" type="xs:long" minOccurs="0" default="1000" />
            <xs:element name="fireAllRulesDuringResynchronization" type="xs:boolean" default="false" minOccurs="0" />
            <xs:element name="fireAllRulesPolicy" type="tns:fireAllRulesPolicy" default="WATCHDOG" />
            <xs:element name="garbageCollectionPeriod" type="xs:long" minOccurs="0" default="10000" />
            <xs:element name="globals" type="tns:globals" />
            <xs:element name="processingMode" type="tns:eventProcessingMode" default="CLOUD" />
            <xs:element name="ruleEngineMode" type="tns:ruleEngineMode" minOccurs="0" default="RETEOO" />
            <xs:element name="rulesFiles" type="tns:rulesFiles" />
            <xs:element name="tickPeriod" type="xs:long" minOccurs="0" default="30000" />
            <xs:element name="customLifeCycleClass" type="xs:string" minOccurs="0" />
            <xs:element name="customInitializationClass" type="xs:string" minOccurs="0" />
            <xs:element name="compressionMode" type="xs:boolean" minOccurs="0" default="false" />
            <xs:element name="compressionPeriod" type="xs:long" minOccurs="0" default="1000" />
            <xs:element name="retractOnResyncPolicy" type="tns:retractOnResyncPolicy" default="PER_FLOW" minOccurs="0" />
        </xs:sequence>
        <xs:attribute name="name" type="xs:string" use="required" />
        <xs:attribute name="__tooltip" type="xs:string" fixed="Scenario Configuration" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_actionRetractedAutomaticallyWhenCompleted" type="xs:string" fixed="true: completed actions are automatically retracted" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_actionRetractedAutomaticallyWhenCompleted" type="xs:string" fixed="true" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_alarmEligibilityPolicy" type="xs:string" fixed="TODO the alarmEligibilityPolicy" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_alarmEligibilityPolicy" type="xs:string" fixed="true" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_asyncActionPeriod" type="xs:string" fixed="Period corresponding to Asynchronous Action Management (callbacks and status updated in rules). Value is in millisecond." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_asyncActionPeriod" type="xs:string" fixed="1000" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_clockTypeMode" type="xs:string" fixed="Not Used" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_clockTypeMode" type="xs:string" fixed="NORMAL" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_eligibleForBroadcast" type="xs:string" fixed="Dispatcher send an incoming alarm only when the Scenario is eligible to broadcast." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_eligibleForBroadcast" type="xs:string" fixed="true" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_fireAllRulePeriod" type="xs:string" fixed="Period of FireAllRule. Linked to fireAllRulePolicy. Value is in millisecond." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_fireAllRulePeriod" type="xs:string" fixed="1000" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_fireAllRulesDuringResynchronization" type="xs:string" fixed="Allows FireAllRules to be triggered during Resynchronization phase." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_fireAllRulesDuringResynchronization" type="xs:string" fixed="false" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>


        <xs:attribute name="__tooltip_fireAllRulesPolicy" type="xs:string" fixed="Policy of the FireAllRule mechanism" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_fireAllRulesPolicy" type="xs:string" fixed="WATCHDOG" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>


        <xs:attribute name="__tooltip_garbageCollectionPeriod" type="xs:string" fixed="Period between Garbage Collecting mechanism (retracting un-eligible Alarms, completed Actions, etc...). Value is in millisecond." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_garbageCollectionPeriod" type="xs:string" fixed="10000" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_processingMode" type="xs:string" fixed="Scenario's Processing mode. Changing this value may impact Rule integrity. It is no recommended to change this value in the GUI." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_processingMode" type="xs:string" fixed="CLOUD" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_ruleEngineMode" type="xs:string"
            fixed="Scenario's Rule Engine mode. Rule Engine mode can be either RETEOO (the algorithm from Drools 5.x selected by default for backward compatibility) or PHREAK (the new algorithm introduced by Drools 6.x). Changing this value may impact Rule integrity. It is no recommended to change this value in the GUI."
            use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_ruleEngineMode" type="xs:string" fixed="RETEOO" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_tickPeriod" type="xs:string" fixed="Period corresponding to the TickFlag update in the Rule. Optional mechanism. Value is in millisecond." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_tickPeriod" type="xs:string" fixed="30000" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_customLifeCycleClass" type="xs:string" fixed="Full Class name of the LifeCycle used to specifically enrich customized Alarms. See UCA development Guide for more information." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_customInitializationClass" type="xs:string" fixed="Full Class name of the Scenario Initialization customization. See UCA development Guide for more information." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_compressionMode" type="xs:string" fixed="Enable the Alarm Compression layer at Scenario's entry point. The Compression layer can save Rules processing when lot of AttributeValueChange or StateChange are received."
            use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_compressionMode" type="xs:string" fixed="false" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_compressionPeriod" type="xs:string" fixed="Buffering Time Window of Compression layer. Compression will analyze and manage Alarms received within this timeframe. Value is in millisecond." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_compressionPeriod" type="xs:string" fixed="1000" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_retractOnResyncPolicy" type="xs:string" fixed="Policy for retracting objects in Working Memory when resynchronizing a mediation flow. Value can be: NONE, PER_FLOW, or ALL." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_retractOnResyncPolicy" type="xs:string" fixed="PER_FLOW" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="filterFiles">
        <xs:sequence>
            <xs:element name="filterFile" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="__tooltip" type="xs:string" fixed="List of Filter Files that will be used for the Scenario." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="mapperFiles">
        <xs:sequence>
            <xs:element name="mapperFile" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="__tooltip" type="xs:string" fixed="List of Mapper Files that will be used for the Scenario." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="globals">
        <xs:sequence>
            <xs:element name="global" type="tns:global" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="__tooltip" type="xs:string" fixed="List of Global Beans that will be available from the Scenario Rules." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="global">
        <xs:sequence>
            <xs:element name="key" type="xs:string" />
            <xs:element name="value" type="xs:string" />
        </xs:sequence>
        <xs:attribute name="__tooltip" type="xs:string" fixed="Beans that is available from the Scenario Rules." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>


        <xs:attribute name="__tooltip_key" type="xs:string" fixed="The name of the Bean defined in the context.xml file." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>

        <xs:attribute name="__tooltip_value" type="xs:string" fixed="The name of the global variable as defined in the Rules of the Scenario" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>


    </xs:complexType>

    <xs:complexType name="rulesFiles">
        <xs:sequence>
            <xs:element name="rulesFile" type="tns:rulesFile" minOccurs="1" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="__tooltip" type="xs:string" fixed="List of Rules Files for the Scenario" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="rulesFile">
        <xs:sequence>
            <xs:element name="filename" type="xs:string" />
            <xs:element name="name" type="xs:string" />
            <xs:element name="paramsFilename" type="xs:string" minOccurs="0" />
            <xs:element name="ruleFileType" type="tns:ruleFileType" />
            <!-- Optional parameters for Decision Tables -->
            <xs:element name="decisionTableType" type="tns:decisionTableType" minOccurs="0" />
            <xs:element name="worksheetName" type="xs:string" minOccurs="0" />
        </xs:sequence>
        <xs:attribute name="disabledAtStartup" type="xs:boolean" default="false" />

        <xs:attribute name="__tooltip" type="xs:string" fixed="Parameters of a Scenario's Rules File." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_filename" type="xs:string" fixed="File path of the Rules File in the form: file:./deploy/vp-2.0/scenario/Rules.drl" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_name" type="xs:string" fixed="Name representing this Rules File" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_paramsFilename" type="xs:string" fixed="TODO File path of the Rules File in the form: file:./deploy/vp-2.0/scenario/Rules.drl" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_ruleFileType" type="xs:string" fixed="Type of the Rules Files" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_decisionTableType" type="xs:string" fixed="Type of the Decision Tables File (XLS or CSV)" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_worksheetName" type="xs:string" fixed="Which named xls worksheet should be used for an XLS Decision Table file. This parameter is optional, and a default worksheet will be used if not specified." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_disabledAtStartup" type="xs:string" fixed="The rules defined in this file will not be active at Scenario startUp. Activation is explicitly required" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_disabledAtStartup" type="xs:string" fixed="false" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_ruleFileType" type="xs:string" fixed="DRL" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_decisionTableType" type="xs:string" fixed="XLS" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="mediationFlows">
        <xs:choice maxOccurs="unbounded">
            <xs:element name="mediationFlow" type="tns:mediationFlow" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="UMBmediationFlow" type="tns:UMBmediationFlow" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="UMBExternalmediationFlow" type="tns:UMBExternalmediationFlow" minOccurs="0" maxOccurs="unbounded" />
        </xs:choice>
        <xs:attribute name="__tooltip" type="xs:string" fixed="List of Mediation Flows for the ValuePack." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="dbFlows">
        <xs:sequence>
            <xs:element name="dbFlow" type="tns:dbFlow" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="__tooltip" type="xs:string" fixed="List of Database Flows for the ValuePack." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="mediationFlow">
        <xs:sequence>
            <xs:element name="flowCreation" type="tns:actionFlow" minOccurs="0" />
            <xs:element name="flowDeletion" type="tns:actionFlow" minOccurs="0" />
            <xs:element name="flowResynchronization" type="tns:actionFlow" />
            <xs:element name="flowStatus" type="tns:actionFlow" />
        </xs:sequence>
        <xs:attribute name="automaticStart" type="xs:boolean" use="optional" default="true" />
        <xs:attribute name="flowNameKey" type="xs:string" use="required" />
        <xs:attribute name="actionReference" type="xs:string" use="required" />
        <xs:attribute name="name" type="xs:string" use="required" />
        <xs:attribute name="lastEventReceivedFirstDuringResynchronization" type="xs:boolean" use="required" />
        <xs:attribute name="__tooltip" type="xs:string" fixed="Parameters of a Mediation Flow." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_automaticStart" type="xs:string" fixed="Boolean flag indicating whether to automatically start the mediation flow when the value pack is started or not." use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__default_automaticStart" type="xs:string" fixed="true" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="__tooltip_name" type="xs:string" fixed="Name of the Mediation Flow" use="optional">
            <xs:annotation>
                <xs:appinfo>
                    <jxb:property fixedAttributeAsConstantProperty="true" />
                </xs:appinfo>
            </xs:annotation>
        </xs:attribute>
